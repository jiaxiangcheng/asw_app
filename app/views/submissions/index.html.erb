<p id="notice"><%= notice %></p>
<% @submissions.each_with_index do |submissions, index| %>
  <!-- <% unless submissions.site == "" %> -->
    <tr class="athing" id="18336202">
      <td class="title" valign="top" align="right" width="23px">
        <span class="rank"><%= index + 1 %>.</span>
      </td>
      <td class="votelinks" valign="top">
        <center>
          <a id="up_18336202" onclick='return vote(event, this, "up")' href="?id=18336202&amp;how=up&amp;auth=82ffa3bbc57e8f84bcfc87c0d5127d61661b2131&amp;goto=news">
            <div class="votearrow" title="upvote"></div>
          </a>
        </center>
      </td>
      <td class="title">
        <a href="#<%= submissions.site %>" class="storylink"><%= submissions.title %></a>
        <span class="sitebit comhead">
          (
          <a href="?site=<%= submissions.site %>">
            <span class="sitestr"><%= submissions.site %></span>
          </a>
          )
        </span>
      </td>
    </tr>
    <tr>
      <td colspan="2"></td>
      <td class="subtext">
        <span class="score" id="score_18336202"><%= submissions.points %> points</span>
        by
        <a href="?id=<%= submissions.author %>" class="hnuser"><%= submissions.author %></a>
        <span class="age">
          <a href="?id=18336202"><%= humanize_time(Time.now() - submissions.created_at) %></a>
        </span>
        <span id="unv_18336202"></span>
        |
        <a href="#" onclick="return hidestory(event, this, 18336202)">hide</a>
        |
        <a href="?id=18336202"><%= submissions.num_comments %>&nbsp;comments</a>
      </td>
    </tr>
    <tr class="spacer" style="height:5px"></tr>
  <!-- <% end %> -->
<% end %>
